SubTitle.TextXAlignment = Enum.TextXAlignment.Left
SubTitle.Parent = MainFrame

local KeyBox = Instance.new("TextBox")
KeyBox.PlaceholderText = "insert your key here"
KeyBox.Text = ""
KeyBox.Font = Enum.Font.Gotham
KeyBox.TextSize = 18
KeyBox.TextColor3 = Color3.fromRGB(255, 255, 255)
KeyBox.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
KeyBox.Size = UDim2.new(0.72, 0, 0, 40)
KeyBox.Position = UDim2.new(0.05, 0, 0.42, 0)
KeyBox.BorderSizePixel = 0
KeyBox.ClearTextOnFocus = false
KeyBox.Parent = MainFrame

local PasteBtn = Instance.new("TextButton")
PasteBtn.Text = "Paste"
PasteBtn.Font = Enum.Font.GothamBold
PasteBtn.TextSize = 18
PasteBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
PasteBtn.BackgroundColor3 = Color3.fromRGB(200, 40, 40)
PasteBtn.Size = UDim2.new(0.2, 0, 0, 40)
PasteBtn.Position = UDim2.new(0.77, 0, 0.42, 0)
PasteBtn.BorderSizePixel = 0
PasteBtn.Parent = MainFrame

PasteBtn.MouseButton1Click:Connect(function()
	local success, text = pcall(function()
		return tostring(setclipboard and getclipboard and getclipboard() or "")
	end)
	if success then
		KeyBox.Text = text
	end
end)

local SubmitBtn = Instance.new("TextButton")
SubmitBtn.Text = "Submit Key  >"
